{
    "version": 2,
    "builds": [
      {
        "src": "web-flask/app.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.9"
        }
      },
      {
        "src": "scraper/scraper.js",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["scraper/**"]
        }
      },
      {
        "src": "web-flask/static/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/web-flask/static/$1"
      },
      {
        "src": "/run_test",
        "methods": ["POST"],
        "dest": "/web-flask/app.py"
      },
      {
        "src": "/(.*)",
        "dest": "/web-flask/app.py"
      }
    ],
    "env": {
      "FLASK_ENV": "production",
      "SECRET_KEY": "<your-secret-key>"
    }
  }
  